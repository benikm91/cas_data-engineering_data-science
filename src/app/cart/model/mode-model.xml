                <mi>y</mi>
                <mo stretchy="false" style="math-style:normal;math-depth:0;">^</mo>
            </mover>
            <msub class="cart-region">
                <mi>R</mi>
                <mi>m</mi>
            </msub>
        </msub>
        <mo>=</mo>
