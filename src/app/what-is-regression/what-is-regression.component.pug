ng-template('#slides'='')
    section(id="{{problemTableOfContentLabel.REGRESSION}}")
        slide-header(header="{{problemTableOfContentLabel.REGRESSION}}")
        div
            p.fragment.fade-up Der Output ist eine kontinuierliche Grösse.
            p.fragment.fade-up Output-Space ist o-Dimensional (meistens 1-Dimensional)
            ul
                li.fragment.fade-up Immobilien Verkaufspreis abschätzen
                li.fragment.fade-up Immobilien Verkaufspreis
            div.r-stack
                div.fragment.fade-in-then-out('#parent'='')
                    ng-container(*ngIf="parent.className.includes('visible')")
                        supervised-model-visualization(input-label="Wohnfläche", output-label="Preis")
                div.fragment.fade-in-then-out('#parent2'='')
                    ng-container(*ngIf="parent2.className.includes('visible')")
                        supervised-model-visualization(input-label="100m²", output-label="10'000 CHF")