                <mrow>
                    <mi>arg</mi>
                    <mspace width="0.167em"></mspace>
                    <mi>max</mi>
                    <mspace width="0.167em"></mspace>
                </mrow>
                <mi class="label">k</mi>
            </munder>
            <mrow title="p: Wahrscheinlichkeit der Klasse k in Region R_m">
                <mi>p</mi>
                <mo form="prefix" stretchy="false">(</mo>
                <mi class="label">y</mi>
                <mo>=</mo>
                <mi class="label">k</mi>
                <mi>|</mi>
                <msub class="cart-region">
                    <mi>R</mi>
                    <mi>m</mi>
                </msub>
                <mo form="postfix" stretchy="false">)</mo>
            </mrow>
        </mrow>
    </mrow>
